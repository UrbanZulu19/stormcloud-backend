# StormCloud Configuration

# API Keys (OpenRouter only - we're cost-conscious)
OPENROUTER_API_KEY=sk-or-v1-your-key-here
STRIPE_SECRET_KEY=sk_test_your-key-here
STRIPE_WEBHOOK_SECRET=whsec_your-secret-here

# Database (Railway auto-fills this)
DATABASE_URL=postgresql://user:pass@localhost:5432/stormcloud

# Redis (for job queue and sessions)
REDIS_URL=redis://localhost:6379

# JWT Secret (generate: openssl rand -hex 32)
JWT_SECRET=your-super-secret-key-minimum-32-chars

# App Config
ENVIRONMENT=development
DEBUG=true
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000

# Code Execution Limits
MAX_EXECUTION_TIME=30
MAX_MEMORY_MB=512
MAX_CPU_CORES=1
ENABLE_NETWORK=false

# Rate Limits
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000
AI_REQUESTS_PER_DAY=1000

# Free Tier Limits
FREE_TIER_EXECUTIONS=100
FREE_TIER_AI_REQUESTS=50
FREE_TIER_STORAGE_MB=100
